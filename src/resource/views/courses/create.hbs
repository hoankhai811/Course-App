<div class='mt-4'>
  <a href="/me/stored/courses">CoursePage</a>
  <h3>Create Course</h3>
  <form method='POST' action='/courses/store'>
    <div class='form-group'>
      <label for='name'>Name</label>
      <input
        class='form-control'
        placeholder='Enter Name'
        id='name'
        name='name'
      />
    </div>
    <div class='form-group'>
      <label for='description'>Description</label>
      <input
        class='form-control'
        placeholder='Enter Description'
        id='description'
        name='description'
      />
    </div>
    <div class='form-group'>
      <label for='videoId'>VideoId</label>
      <input
        class='form-control'
        placeholder='Enter VideoId'
        id='videoId'
        name='videoId'
      />
    </div>
    <div class='form-group'>
      <label for='level'>Level</label>
      <input
        class='form-control'
        placeholder='Enter Level'
        id='level'
        name='level'
      />
    </div>
    <div class='form-group'>
      <label for='imageHome'>Image Home</label>
      <input
        class='form-control'
        placeholder='Enter ImageHome'
        id='imageHome'
        name='imageHome'
      />
    </div>
    <button type='submit' class='btn btn-primary'>Thêm Khoá Học</button>
  </form>
</div>